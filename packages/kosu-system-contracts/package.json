{
    "name": "@kosu/system-contracts",
    "version": "0.1.5",
    "description": "Kosu protocol core contracts in Solidity.",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/ParadigmFoundation/kosu-monorepo/blob/master/packages/kosu-system-contracts"
    },
    "main": "dist/src/index.js",
    "types": "src",
    "bugs": {
        "url": "https://github.com/ParadigmFoundation/kosu-monorepo/issues"
    },
    "scripts": {
        "test": "yarn build && ts-mocha -p test/tsconfig.json test/* --timeout 0 --slow 2000",
        "contracts:test:ci": "ts-mocha -p test/tsconfig.json test/* --slow 2000 --timeout 0 -b -- geth",
        "coverage": "ts-mocha -p test/tsconfig.json test/* --slow 2000 --timeout 100000 -- runCoverage && istanbul report html && open coverage/index.html",
        "build": "run-s compile:solidity generate_contract_wrappers compile:typescript",
        "compile:solidity": "sol-compiler",
        "compile:typescript": "tsc",
        "lint": "tslint -p .",
        "truffle:clean": "shx rm -rf build/contracts",
        "contracts:clean": "shx rm -rf  generated-artifacts generated-wrappers",
        "deploy": "yarn deploy:ropsten",
        "deploy:ropsten": "ts-node -T src/bin/migrate.ts --rpc-url=https://ethnet.zaidan.io/ropsten && ts-node --files src/bin/deliver_tokens.ts --rpc-url=https://ethnet.zaidan.io/ropsten &&yarn prettier:deployedAddresses",
        "deploy:kosu": "ts-node -T src/bin/migrate.ts --test-mnemonic --rpc-url=https://ethnet.zaidan.io/kosu && ts-node --files src/bin/deliver_tokens.ts --test-mnemonic --rpc-url=https://ethnet.zaidan.io/kosu && yarn prettier:deployedAddresses",
        "deploy:kosu:ci": "ts-node -T src/bin/migrate.ts --test-mnemonic --rpc-url=${WEB3_URI} && ts-node --files src/bin/deliver_tokens.ts --test-mnemonic --rpc-url=${WEB3_URI} && yarn prettier:deployedAddresses",
        "deploy:0x:kosu": "0x-migrate --rpc-url https://ethnet.zaidan.io/kosu --from 0x5409ED021D9299bf6814279A6A1411A7e866A631",
        "deploy:0x:kosu:ci": "0x-migrate --rpc-url ${WEB3_URI} --from 0x5409ed021d9299bf6814279a6a1411a7e866a631",
        "migrate:zaidan": "run-s deploy:kosu deploy:0x:kosu",
        "migrate:ci": "run-s deploy:kosu:ci deploy:0x:kosu:ci",
        "prettier:deployedAddresses": "prettier --write src/deployedAddresses.json --config ../../.prettierrc",
        "ganache_rpc": "docker run --rm -p 8545:8545 ${npm_package_config_kosu_ganache_image}",
        "ganache_rpc:detached": "docker run --rm -d -p 8545:8545 ${npm_package_config_kosu_ganache_image}",
        "generate_contract_wrappers": "abi-gen --abis  ${npm_package_config_abis} --template ./abi-gen-templates/contract.handlebars --partials './abi-gen-templates/partials/**/*.handlebars' --output generated-wrappers --backend ethers",
        "docs": "node devdoc.js ./generated-artifacts ./docs "
    },
    "config": {
        "kosu_ganache_image": "gcr.io/kosu-io/kosu-ganache",
        "test_mnemonic": "plate tag lend tissue capable ketchup evidence deliver aspect salt used always",
        "abis:comment": "This list is auto-generated by contracts-gen. Don't edit manually.",
        "abis": "./generated-artifacts/@(AuthorizedAddresses|BasicTradeSubContract|EventEmitter|KosuToken|OrderGateway|PosterRegistry|PosterRegistryProxy|Treasury|ValidatorRegistry|Voting|ZeroExV2SubContract).json"
    },
    "dependencies": {
        "@0x/base-contract": "^5.0.5",
        "@0x/contract-addresses": "^2.3.3",
        "@0x/contracts-exchange": "^2.1.5",
        "@0x/subproviders": "^4.1.0",
        "@0x/utils": "^4.3.1",
        "@0x/web3-wrapper": "^6.0.5",
        "@kosu/subcontract-sdk": "0.1.1",
        "@kosu/tsc-config": "^0.1.0",
        "@kosu/tslint-config": "^0.0.4",
        "bitcore-lib": "0.16",
        "ethereum-types": "^2.1.2",
        "openzeppelin-solidity": "2.1.2",
        "safe-node-require": "^1.0.0",
        "web3": "1.0.0-beta.37",
        "web3-eth-abi": "1.0.0-beta.37",
        "web3-provider-engine": "^15.0.0",
        "web3-utils": "1.0.0-beta.37"
    },
    "devDependencies": {
        "0x.js": "^6.0.9",
        "@0x/abi-gen": "^2.0.9",
        "@0x/abi-gen-templates": "^2.0.2",
        "@0x/contracts-gen": "^1.0.8",
        "@0x/contracts-test-utils": "^3.1.2",
        "@0x/dev-utils": "^2.2.2",
        "@0x/migrations": "^4.1.6",
        "@0x/sol-compiler": "^3.1.6",
        "@0x/sol-coverage": "^3.0.4",
        "@0x/sol-profiler": "^3.1.6",
        "@0x/sol-trace": "^2.0.12",
        "@babel/core": "^7.4.4",
        "@types/chai-as-promised": "^7.1.0",
        "@types/mocha": "^5.2.6",
        "chai": "^4.2.0",
        "chai-as-promised": "^7.1.1",
        "ethereumjs-abi": "^0.6.7",
        "ethereumjs-util": "^6.1.0",
        "istanbul": "^0.4.5",
        "json2md": "^1.6.3",
        "truffle-hdwallet-provider": "^1.0.6",
        "typescript": "^3.4.5",
        "yargs": "^13.2.2"
    },
    "publishConfig": {
        "access": "public"
    },
    "files": [
        "dist/",
        "contracts/",
        "compiler.json",
        "tsconfig.json",
        "tslint.json"
    ]
}
